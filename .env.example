# Neo4j Aura Connection
NEO4J_URI=neo4j+s://xxxxx.databases.neo4j.io
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_password_here
NEO4J_DATABASE=neo4j
AURA_INSTANCEID=xxxxx
AURA_INSTANCENAME=Instance Name

# AWS Aurora PostgreSQL (to be added later)
POSTGRES_HOST=
POSTGRES_PORT=5432
POSTGRES_DATABASE=
POSTGRES_USERNAME=
POSTGRES_PASSWORD=

# ClickHouse Cloud (to be added later)
CLICKHOUSE_HOST=
CLICKHOUSE_PORT=8443
CLICKHOUSE_DATABASE=
CLICKHOUSE_USERNAME=
CLICKHOUSE_PASSWORD=

# Pinecone (Vector DB + Embeddings)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1-aws
PINECONE_INDEX_NAME=engineering-intelligence

# Featherless AI (for agent LLM)
FEATHERLESS_API_KEY=your_featherless_api_key_here
FEATHERLESS_MODEL=Qwen/Qwen3-32B

# Apache Kafka / AWS MSK
# For local development:
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPICS=engineering-events
KAFKA_GROUP_ID=database-agent-group
KAFKA_SECURITY_PROTOCOL=PLAINTEXT
KAFKA_SSL_ENABLED=false

